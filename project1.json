{
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "dekkov",
            "ActionRepoName": "FaaSr-cli",
            "Branch": "main",
            "Token": "My_GitHub_Account_TOKEN"
        },
        "My_Lambda_Account": {
            "FaaSType": "Lambda",
            "Region": "us-east-1",
            "AccessKey": "My_Lambda_Account_ACCESS_KEY",
            "SecretKey": "My_Lambda_Account_SECRET_KEY"
        }
    },
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "hoang",
            "Region": "us-east-1",
            "Writable": "TRUE",
            "AccessKey": "My_Minio_Bucket_ACCESS_KEY",
            "SecretKey": "My_Minio_Bucket_SECRET_KEY"
        }
    },
    "FunctionList": {
        "start_github": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "project1",
                "output1": "sample1.csv",
                "output2": "sample2.csv"
            },
            "InvokeNext": [
                "sum_github",
                "start_lambda"
            ]
        },
        "start_lambda": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_Lambda_Account",
            "Arguments": {
                "folder": "project1",
                "output1": "sample1_lambda.csv",
                "output2": "sample2_lambda.csv"
            },
            "InvokeNext": "sum_lambda"
        },
        "sum_github": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "project1",
                "input1": "sample1.csv",
                "input2": "sample2.csv",
                "output": "sum.csv"
            },
            "InvokeNext": [

            ]
        },
        "sum_lambda": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_Lambda_Account",
            "Arguments": {
                "folder": "project1",
                "input1": "sample1_lambda.csv",
                "input2": "sample2_lambda.csv",
                "output": "sum.csv"
            },
            "InvokeNext": [

            ]
        }
    },
    "ActionContainers": {
        "start_github": "ghcr.io/nolcut/ga-rpc-python-base-r:dev",
        "sum_github": "ghcr.io/nolcut/ga-rpc-python-base-r:dev"
    },
    "FunctionGitRepo": {
        "create_sample_data": "FaaSr/FaaSr-tutorial",
        "compute_sum": "FaaSr/FaaSr-tutorial"
    },
    "FunctionInvoke": "start_github",
    "InvocationID": "",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}
