{
    "WorkflowName": "project2",
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "dekkov",
            "ActionRepoName": "FaaSr-workflow",
            "Branch": "main",
            "Token": "My_GitHub_Account_TOKEN"
        },
        "My_Lambda_Account": {
            "FaaSType": "Lambda",
            "Region": "us-east-1",
            "AccessKey": "My_Lambda_Account_ACCESS_KEY",
            "SecretKey": "My_Lambda_Account_SECRET_KEY"
        }
    },
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "https://play.min.io",
            "Bucket": "hoang",
            "Region": "us-east-1",
            "Writable": "TRUE",
            "AccessKey": "My_Minio_Bucket_ACCESS_KEY",
            "SecretKey": "My_Minio_Bucket_SECRET_KEY"
        }
    },
    "ActionList": {
        "start": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_GitHub_Account",
            "Type": "R",
            "Arguments": {
                "folder": "project2",
                "output1": "df1.csv",
                "output2": "df2.csv"
            },
            "InvokeNext": [
                "sum"
            ]
        },
        "sum": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_GitHub_Account",
            "Type": "R",
            "Arguments": {
                "folder": "project2",
                "input1": "df1.csv",
                "input2": "df2.csv",
                "output": "sum.csv"
            },
            "InvokeNext": []
        }
    },
    "ActionContainers": {
        "start": "ghcr.io/nolcut/ga-rpc-python-base-r:dev",
        "sum": "ghcr.io/nolcut/ga-rpc-python-base-r:dev"
    },
    "FunctionGitRepo": {
        "create_sample_data": "FaaSr/FaaSr-tutorial",
        "compute_sum": "FaaSr/FaaSr-tutorial"
    },
    "FunctionInvoke": "start",
    "InvocationID": "",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}
